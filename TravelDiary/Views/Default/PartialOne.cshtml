@using TravelDiary.Models.Classes
@model List<Blog>

<div class="con-bot">
    @foreach (var x in Model)
    {
        <div class="left">
            <div class="bot-left">
                <img src="~/web/images/pic.jpg" alt="" />
            </div>
            <div class="left-text">
                <h4>@x.Title</h4>
                <span class="doller">@x.History</span>
                <p>@x.Explanation</p>
                <p><a href="/Blog/BlogDetail/@x.ID"><span class="read">Read More >></span></a></p>
            </div>
        </div>
    }
    @Html.Action("PartialTwo", "Default")


    <div class="clear"></div>
</div>

